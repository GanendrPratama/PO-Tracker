{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "sql:default",
    "sql:allow-execute",
    "sql:allow-select",
    "barcode-scanner:default",
    "dialog:default",
    "fs:default",
    "fs:allow-appdata-write-recursive",
    "fs:allow-appdata-read-recursive",
    "core:path:default"
  ]
}