{
  "name": "tauri-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:microservice\"",
    "dev:frontend": "vite",
    "dev:microservice": "cd microservice && npm run dev",
